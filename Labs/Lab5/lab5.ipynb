    "# The following code that allow us to run multiprocessing tasks on Jupyter is taken from \n",
    "#https://stackoverflow.com/questions/50937362/multiprocessing-on-python-3-jupyter \n",
    "import os\n",
    "import random\n",
    "def is_notebook() -> bool:\n",
    "    try:\n",
    "        if \"get_ipython\" in globals().keys():\n",
    "            shell = get_ipython().__class__.__name__\n",
    "            if shell == \"ZMQInteractiveShell\":\n",
    "                return True  # Jupyter notebook or qtconsole\n",
    "            else:\n",
    "                return False  # Other type (?)\n",
    "        else:\n",
    "            return False\n",
    "    except NameError:\n",
    "        return False  # Probably standard Python interpreter\n",
    "\n",
    "\n",
    "if is_notebook():\n",
    "    from multiprocessing.pool import ThreadPool as Pool\n",
    "    from threading import Lock\n",
    "else:\n",
    "    from multiprocessing.pool import Pool\n",
    "    from multiprocessing import Lock\n",
    "\n",
    "# The main code\n",
    "def generate_values_for_parallel(max: int):\n",
    "    # Sample sets of parameters for the Markov chain\n",
    "    parameters = [\n",
    "        (np.array([0.7, 0.3]), np.array([[0.8, 0.2], [0.4, 0.6]]), 10),\n",
    "        (np.array([0.6, 0.4]), np.array([[0.9, 0.1], [0.5, 0.5]]), 10),\n",
    "        (np.array([0.5, 0.5]), np.array([[0.7, 0.3], [0.6, 0.4]]), 10)\n",
    "    ]\n",
    "\n",
    "    for _ in range(max):\n",
    "        yield random.choice(parameters)\n",
    "\n",
    "def parallel_unit(args):\n",
    "    rho, A, nmax = args\n",
    "    rng = np.random.default_rng()\n",
    "    result = markov(rho, A, nmax, rng)\n",
    "    return result\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    results = []\n",
    "    pool = Pool(processes=4)\n",
    "    for loop_result in pool.imap_unordered(parallel_unit, generate_values_for_parallel(10), 2*os.cpu_count()):\n",
    "        results.append(loop_result)\n",
    "    pool.close()\n",
    "    pool.join()\n",
    "    for res in results:\n",
    "        print(res)\n",
    "\n"
